import turtle

turtle.forward(100)
turtle.right(90)
turtle.forward(100)
turtle.right(90)
turtle.forward(100)
turtle.right(90)
turtle.forward(100)
turtle.right(90)

turtle.setpos(0,-100)
turtle.backward(100)
turtle.left(45)
turtle.goto(0,0)

turtle.right(135)
turtle.forward(100)
turtle.left(90)

turtle.forward(200)
turtle.left(45)
turtle.goto(100,0)
